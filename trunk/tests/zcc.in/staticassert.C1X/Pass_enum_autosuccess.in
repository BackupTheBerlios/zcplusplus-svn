SUFFIXES h hpp
OBJECTLIKE_MACRO STATIC_ASSERT _Static_Assert static_assert
// (C)2010 Kenneth Boyd, license: MIT.txt

enum test {
	neg_one = -1,
	zero,
	one
}

STATIC_ASSERT(neg_one,"automatic success has failed");
STATIC_ASSERT(one,"automatic success has failed");

// check unary +
STATIC_ASSERT(+neg_one,"automatic success has failed");
STATIC_ASSERT(+one,"automatic success has failed");

// check unary -
STATIC_ASSERT(-neg_one,"automatic success has failed");
STATIC_ASSERT(-one,"automatic success has failed");

